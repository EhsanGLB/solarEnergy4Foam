FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            powerLawRadialVelocity4SEFoam;
        Vm              (0.0935);//Re=10000 for T=400K
        R               0.038;
        m               6;
        longDir         (0 1 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    tubeWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


