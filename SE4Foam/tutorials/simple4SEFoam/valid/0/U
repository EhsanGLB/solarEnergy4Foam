FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0.1421 0);//Re=1000, Pe^1/3=18

        /*type            powerLawRadialVelocity4SEFoam;
        Vm              (0.3875);//Re=10000
        R               0.04;
        m               6;
        longDir         (0 1 0);*/
    }

    outlet
    {
        type            zeroGradient;
    }

    tubeWall
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


